.stName{
  display: block;
  font-family: HYQiHeiY1-DEW;
  font-size: 20rpx;
  color: rgb(170, 170, 170);
}
.time{
  display: block;
  font-family: HYQiHeiY1-DEW;
  font-size: 20rpx;
  color: rgb(170, 170, 170);
}
.content{
  height: 58%;
  overflow: scroll;
  display: block;
  font-family: HYQiHeiY1-DEW;
  font-size: 26rpx;
  color: rgb(76, 76, 76);
  margin: 20rpx 60rpx 0 60rpx;
  word-wrap: break-word;
  line-height: 50rpx;
  letter-spacing: 3rpx;
  position: relative;
  padding-bottom: 8%;
}
.pullCtn{
  width: 100%;
  height: 90rpx;
  text-align: center;
  /* font-family: MicrosoftYaHei;
  font-size: 22rpx;
  color: #b0b0b0; */
  /* background-color: white; */
  position: absolute;
  top: 73%;
  align-items: flex-end;
  -webkit-box-align: end;
  -webkit-align-items:flex-end;
  background: linear-gradient(to top, rgba(255,255,255,1),rgba(255,255,255,1),rgba(255,255,255,0.9),rgba(255,255,255,0.7),rgba(255,255,255,0.5));
  /* padding-bottom: 15rpx; */
}
.btn0{
  width: 180rpx;
  height: 60rpx;
  border: 1px solid rgb(255, 210, 0);
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  color: rgb(255, 210, 0);
  font-family: HYQiHeiY1-DEW;
  font-size: 30rpx;
  position: absolute;
  bottom: 30rpx;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
  z-index: 8;
  background-color: white;
}
.btn1{
  width: 180rpx;
  height: 60rpx;
  border: 1px solid rgb(201,201,201);
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  color: rgb(201,201,201);
  font-family: HYQiHeiY1-DEW;
  font-size: 30rpx;
  position: absolute;
  bottom: 30rpx;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
  background-color: white;
}
.shadow{
  height: 80rpx;
  width: 180rpx;
  position: absolute;
  bottom: -10rpx;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
  z-index: 6;
}

.bottom{
  height: 40rpx;
  width: 100%;
  background-color: #fff;
  border-top-left-radius: 28rpx;
  border-top-right-radius: 28rpx;
  box-shadow: -4rpx 0rpx 12rpx rgba(0, 0, 0, 0.2);
  color: rgb(170, 170, 170);
  font-family: HYQiHeiY1-DEW;
  font-size: 21rpx;
  text-align: center;
  line-height: 40rpx;
  position: fixed;
  bottom: 0;
}
.contact{
  border-bottom:1px solid rgb(201,201,201);
  width:80%;
  margin-left: 10%;
}
.coorperation{
  font-family: HYQiHeiY1-DEW;
  font-size: 26rpx;
  color: rgb(76, 76, 76);
  margin-top: 20rpx;
}
scroll-view{
  height: 100%;
  white-space: pre-wrap;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.stItem{
  display: inline-block;
  width: 300rpx;
  height: 265rpx;
  box-shadow: 14rpx 14rpx 32rpx rgba(0, 0, 0, 0.2);
  border-radius: 20rpx;
  background-color: #fff;
  position: relative;
  margin-top: 40rpx;
  margin-left: 50rpx;
  text-align: center;
}
.stPhoto{
  height: 148rpx;
  width: 80%;
  margin: 0 auto;
  padding-top: 17rpx;
  padding-bottom: 7rpx;
}
.stPhoto image{
  height: 148rpx;
  width: 100%;
}
.name{
  display: inline-block;
  text-align: center;
  font-size: 24rpx;
  letter-spacing: 2rpx;
  width: 230rpx;
  height: 65rpx;
  word-wrap: break-word;
  margin: 0 auto;
  overflow: hidden;
}
.area{
  width: 100rpx;
  display: block;
  font-family: HYQiHeiY1-DEW;
  font-size: 20rpx;
  color: rgb(196, 196, 196);
  text-align: center;
  position: absolute;
  bottom: 6rpx;
  left: 0;
  right: 0;
  margin: auto;
}
.tip{
  font-family: HYQiHeiY1-DEW;
  font-size: 20rpx;
  color: rgb(170, 170, 170);
  text-align: center;
  margin: 80rpx 0 60rpx 0;
}