/* pages/mine/mine.wxss */
page{
  background-color: rgb(242, 242, 242);
}
.container{
  width: 100%;
}
.top{
  width: 100%;
  border-bottom-left-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  box-shadow: 4rpx 4rpx 12rpx rgba(0, 0, 0, 0.2);
  position: fixed;
  top: 0;
  z-index: 90;
  background: #fff url(data:image/png;base64,) no-repeat fixed top;
  background-size:contain;
}
.topimg{
  width: 750rpx;
  position: absolute;
  height: 250rpx;
  top: 0;
}
.head{
  display: block;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin: 10rpx auto 20rpx auto;
  padding: 0px;
}
.username{
  display: block;
  font-family: MicrosoftYaHei;
  color: rgb(76, 76, 76);
  font-size: 26rpx;
}
.topBtn{
  width: 100%;
}
.notice,.like{
  width: 50%;
  align-items: flex-start;
  padding-top: 20rpx;
}
.notice image{
  width: 30rpx;
  height: 28rpx;
}
.like image{
  width: 26rpx;
  height: 30rpx;
}
.like image,.notice image{
  padding-top: 5rpx;
  display: block;
}
.is_checked{
  display: block;
  width: 52rpx;
  font-family: HYQiHeiY1-DEW;
  font-size: 26rpx;
  color: rgb(255, 210, 0);
  margin: 0 0 0 10rpx;
  padding-bottom: 21rpx;
  border-bottom: 4rpx solid rgb(255, 210, 0);
}
.normal{
  display: block;
  width: 52rpx;
  font-family: HYQiHeiY1-DEW;
  font-size: 26rpx;
  color: rgb(196, 196, 196);
  margin: 0 0 0 10rpx;
  padding-bottom: 21rpx;
  border-bottom: 4rpx solid rgb(196, 196, 196);
}
.middle{
  overflow: hidden;
  margin-top: 38%;
}
swiper{
  position: absolute;
  height: 70%;
  width: 100%;
  margin-top: 40rpx;
}
.item{
  width: 500rpx;
  height: 90%;
  border-radius: 20rpx;
  background-color: #fff;
  box-shadow: 10rpx 20rpx 20rpx rgba(0, 0, 0, 0.1);
  position: relative;
  margin-left: 80rpx;
}
.itemTop{
  border-bottom: 1px solid rgba(0, 0, 0, 0.2);
}
.title{
  font-family: HYQiHeiY1-DEW;
  font-size: 30rpx;
  color: rgb(0, 0, 0);
  display: block;
  padding-top: 30rpx;
  padding-left: 56rpx;
}
.itemTopBottom{
  padding: 15rpx 40rpx;
}
